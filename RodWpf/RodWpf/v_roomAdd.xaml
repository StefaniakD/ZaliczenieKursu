<Window x:Class="RodWpf.v_roomAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RodWpf"
        mc:Ignorable="d"
        Title="v_roomAdd" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="tbxRoomNumber" HorizontalAlignment="Left" Height="23" Margin="142,95,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Numer kwatery (*) :" HorizontalAlignment="Left" Margin="30,96,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.142,0.179"/>
        <Button x:Name="buttonSave" Content="Save" HorizontalAlignment="Left" Margin="141,172,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="buttonSave_Click"/>

    </Grid>
</Window>
